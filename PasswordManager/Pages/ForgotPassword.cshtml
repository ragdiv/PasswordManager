
@page
@model PasswordManager.Pages.ForgotPasswordModel
@{
    ViewData["Title"] = "ForgotPassword";
}
<br />
<br />

<form method="post">
    <div class="form-group">
        <label for="exampleInputEmail1">Email address</label>
        <input type="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp">
        <small id="emailHelp" class="form-text text-muted">Enter the email address associated with your account to reset.</small>
    </div>

    <button type="submit" class="btn btn-primary">Reset</button><br /><br />


</form>